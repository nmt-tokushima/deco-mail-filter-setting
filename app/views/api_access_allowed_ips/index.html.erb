<p id="notice"><%= notice %></p>

<h1>Api Access Allowed Ips</h1>

<table>
  <thead>
    <tr>
      <th>Ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_access_allowed_ips.each do |api_access_allowed_ip| %>
      <tr>
        <td><%= api_access_allowed_ip.ip %></td>
        <td><%= link_to 'Show', api_access_allowed_ip %></td>
        <td><%= link_to 'Edit', edit_api_access_allowed_ip_path(api_access_allowed_ip) %></td>
        <td><%= link_to 'Destroy', api_access_allowed_ip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Access Allowed Ip', new_api_access_allowed_ip_path %>
