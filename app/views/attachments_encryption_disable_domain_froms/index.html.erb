<h1>Attachments Encryption Disable Domain Froms</h1>

<table class="table">
  <thead>
    <tr>
      <th>Domain</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @attachments_encryption_disable_domain_froms.each do |attachments_encryption_disable_domain_from| %>
      <tr>
        <td><%= attachments_encryption_disable_domain_from.domain %></td>
        <td><%= link_to 'Edit', edit_attachments_encryption_disable_domain_from_path(attachments_encryption_disable_domain_from), class: 'btn' %></td>
        <td><%= link_to 'Destroy', attachments_encryption_disable_domain_from, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-error' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attachments Encryption Disable Domain From', new_attachments_encryption_disable_domain_from_path, class: 'btn' %>
